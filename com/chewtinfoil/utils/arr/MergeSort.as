package com.chewtinfoil.utils.arr {		import com.chewtinfoil.utils.ArrayUtils;	public class MergeSort {				public static function sort(p_arr:Array, ...args):void {						var low:uint = (args[0] == undefined) ? 0 : args[0];			var high:uint = (args[1] == undefined) ? p_arr.length-1 : args[1];						if (low == high) { return; }						var p:Number = (low+high) / 2 <<0;						sort(p_arr, low, p);			sort(p_arr, p+1, high);						var l:Number = high-low+1;						var working:Array = new Array(l);						for (var i:Number=0; i<l; i++) { working[i] = p_arr[low+i]; }						var m1:Number = 0;			var m2:Number = p-low+1;						for (var i:Number=0; i <l; i++) {				if (m2 <= high - low) {					if (m1 <= p - low) {						if (working[m1] > working[m2]) {							p_arr[i + low] = working[m2++];						} else {							p_arr[i + low] = working[m1++];						}					} else {						p_arr[i + low] = working[m2++];					}				} else {					p_arr[i + low] = working[m1++];				}			}		}			}}